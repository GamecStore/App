<body class="bg-black">
    <!-- Header -->
    <header class="p-3 menu-fixed bg">
        <%- include ('../partials/header.ejs') %>
    </header>

    <!-------body-------> 
    <div class="container mb-5">
        <div class="row">
            <div class="col-8 mt-5">
                <div class="row row-cols-2 row-cols-md-2 row-cols-lg-3 g-3">
                    <% let count = 0%>
                    <% gamesArray.forEach(element => { %>
                        <% count++ %>
                        <div class="col">
                            <div class="card w-100 h-100">
                                <% const p = element.poster %>
                                <%= p %>
                                <img src="../../public/images/uploads/<% element.poster %>"
                                class="card-img-top" alt="<%=element.gameTitle %> poster" >
                                <div class="card-body">
                                    <h5 class="card-title"><%= element.gameTitle %></h5>
                                    <p class="card-text"><%= element.price %></p>
                                </div>
                            </div>
                        </div>
                    <%});%>
                    <% if(count == 0) { %>
                        <p class ="text-center">No data to show</p>
                    <% } %>
                </div>
            </div> 




            
